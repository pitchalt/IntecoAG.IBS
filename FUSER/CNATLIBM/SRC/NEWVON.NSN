         DECIDE ON FIRST VALUE CODERR
          VALUE 0
           MESSAGE := ' '
          VALUE 1
           MESSAGE := 'Такой банк не зарегистрирован.'
           POST := POS(SCRBNK.BN_BIC)
          VALUE 2
