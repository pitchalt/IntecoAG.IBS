DEFINE SUBROUTINE PROC-CHEK-ONE
RESET WORK-ISN
IF ZKI = ' ' THEN
   RE-MSG := 'Заказ ?' CCC := POS(ZKI) ESCAPE TOP
END-IF
Z. FIND ZAKAZ ZK-F = ZKI
   WORK-ISN := *ISN(Z.)
   TEI := TE-F(Z.) BSI := BS-F(Z.) FDV := FD-F(Z.)
   LHI := NM-F(Z.) GZK := GZ-F(Z.) DTO := DT-OT(Z.)
   END-FIND
DECIDE ON EVERY VALUE REG
VALUES 1
   IF *NUMBER(Z.) > 0 THEN
      RE-MSG := 'Зaкaз oткpыт paнee' CCC := POS(ZKI)
      ESCAPE TOP END-IF
   IF BSI = 0 THEN
IGNORE
*     RE-MSG := 'Cубсчет ?' CCC := POS(BSI) ESCAPE TOP
   ELSE
      KEYKD := BSI PERFORM CHEK-BSH
      IF RE-MSG NE ' ' THEN CCC := POS(BSI) ESCAPE TOP END-IF
   END-IF
   IF DTO = 0 THEN DTO := *DATN
   ELSE
      IF DTO NE MASK(YYYYMMDD) OR DTO = MASK(0) THEN
         RE-MSG := 'Дата открыти  ?' CCC := POS(DTO)
         ESCAPE TOP END-IF
   END-IF
   IF NOT ( NDSI = 'О' OR = 'Р' OR = 'Н' )
     RE-MSG := 'Укажите О или Р или Н'  CCC := POS(NDSI)
   END-IF
   IF GZK NE 0 THEN
*      KEYKD := GZK PERFORM CHEK-GZ
      IF RE-MSG NE ' ' THEN CCC := POS(GZK) ESCAPE TOP END-IF
   END-IF
VALUES 2
   IF *NUMBER(Z.) = 0 THEN
      RE-MSG := 'Указанный заказ не открыт' CCC := POS(ZKI)
      ESCAPE TOP END-IF
   IF DTZ = 0 THEN DTZ := *DATN
   ELSE
      IF DTZ = MASK(0) OR DTZ NE MASK(YYYYMMDD) THEN
         RE-MSG := 'Дата закрыти  ?' CCC := POS(DTZ)
         ESCAPE TOP END-IF
   END-IF
NONE VALUES IGNORE
END-DECIDE
END-SUBROUTINE
