DEFINE SUBROUTINE PROC-LOAD
CALL 'GTSX' L-DEL-B Q-DEL-B N-DEL-B
RESET M-EL F-EL M-DEL FLG
IF ZKI NE ' '
  FIND ZAKAZ ZK-F = ZKI OR ZZ-F = ZKI
  Q-DATA-B := *NUMBER + 1 STEK-ISN := *ISN
  PERFORM PROC-STEK
  END-FIND
ELSE
  IF BSI NE 0
    FIND ZAKAZ BS-F = BSI
    Q-DATA-B := *NUMBER + 1 STEK-ISN := *ISN
    PERFORM PROC-STEK
    END-FIND
  ELSE
    IF TEI NE ' ' THEN
      FIND ZAKAZ TE-F = TEI
      Q-DATA-B := *NUMBER + 1 STEK-ISN := *ISN
      PERFORM PROC-STEK
      END-FIND
    ELSE
      IF MOI NE 0
        FIND ZAKAZ MO-F = MOI
        Q-DATA-B := *NUMBER + 1 STEK-ISN := *ISN
        PERFORM PROC-STEK
        END-FIND
      ELSE  Q-DATA-B := 5000
        READ ZAKAZ
        STEK-ISN := *ISN PERFORM PROC-STEK
        END-READ
      END-IF
    END-IF
  END-IF
END-IF
IF M-EL NE 0 THEN
  C-DATA1-B := T-KEY C-DATA2-B := L-KEY
  CALL 'SRTX' C-DATA1-B C-DATA2-B N-DATA-B
END-IF
CCC := 1
END-SUBROUTINE
