    END-IF
    IF #FIELD-03(C-LINE) = 0
      REINPUT ' ”–—' MARK *#FIELD-03(C-LINE) ALARM
    END-IF
    IF #FIELD-04(C-LINE) = 0  #FIELD-04(C-LINE) := 1  END-IF
    ELSE                                          /* —“–Œ ¿ ”ƒ¿À≈Õ¿
    IF C-DATA > MAX-EL(N-DATA) THEN ESCAPE BOTTOM END-IF
  END-IF
*
END-FOR
