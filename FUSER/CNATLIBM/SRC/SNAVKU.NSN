DEFINE SUBROUTINE LOAD-PROC
*
ASSIGN C-DATA = 0
  FIND IBS-VKU-FILE OG-VA-DT = KEY1 THRU KEY2
    IF C-DATA > Q-DATA THEN ESCAPE BOTTOM END-IF
